{
  "name": "libridex_audiobook_shuffle",
  "version": "1.0.0",
  "description": "Audiobook Shuffling App",
  "main": "server.js",
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "start": "sudo ./scripts/preflight.sh && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "sudo node ./tests/login_pp_test.js"
  },
  "author": "Joseph Edward",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.6.2",
    "axios": "^0.18.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.10",
    "morgan": "^1.9.1",
    "node": "^10.16.1",
    "nodemon": "^1.18.7",
    "npm": "^6.11.2",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "proxyquire": "^2.1.3",
    "qs": "^6.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-sticky-footer": "0.1.0-rc3",
    "react-textarea-autosize": "^7.1.0",
    "request": "^2.88.0",
    "semantic-ui-react": "^0.87.3",
    "sinon": "^8.1.0",
    "sinon-chai": "^3.4.0",
    "typescript": "^3.5.3",
    "validator": "^11.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}
